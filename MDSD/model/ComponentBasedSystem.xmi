<?xml version="1.0" encoding="UTF-8"?>
<metaModel:ComponentBasedSystem
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:metaModel="http://metaModel"
    xmlns:metaModel.viewType.assembly="http://metaModel/viewType/assembly"
    xmlns:metaModel.viewType.repository="http://metaModel/viewType/repository"
    xsi:schemaLocation="http://metaModel metamodel.ecore http://metaModel/viewType/assembly metamodel.ecore#//viewType/assembly http://metaModel/viewType/repository metamodel.ecore#//viewType/repository">
  <repositories>
    <components
        name="WebBrowser"
        requires="//@repositories.0/@interface.0"/>
    <components
        name="WebGUI"
        requires="//@repositories.0/@interface.3"/>
    <components
        xsi:type="metaModel.viewType.assembly:CompositeComponent"
        name="MediaStore"
        requires="//@repositories.0/@interface.2"
        provides="//@repositories.0/@interface.3"
        encapsulatedInstances="//@systems.0/@encapsulatedInstances.2 //@systems.0/@encapsulatedInstances.3 //@systems.0/@encapsulatedInstances.1"/>
    <components
        name="MediaManager"
        requires="//@repositories.0/@interface.1 //@repositories.0/@interface.2"/>
    <components
        name="DigitalWatermarking"
        provides="//@repositories.0/@interface.1"/>
    <components
        name="DBCache"
        requires="//@repositories.0/@interface.2"
        provides="//@repositories.0/@interface.2"/>
    <components
        name="PoolingAudioDB"
        provides="//@repositories.0/@interface.2"/>
    <interface
        name="HTTP">
      <signatures
          name="HTTPDownload">
        <returnType
            xsi:type="metaModel.viewType.repository:VoidType"/>
      </signatures>
      <signatures
          name="HTTPUpload">
        <returnType
            xsi:type="metaModel.viewType.repository:VoidType"/>
      </signatures>
    </interface>
    <interface
        name="Sound">
      <signatures
          name="Watermark">
        <returnType
            xsi:type="metaModel.viewType.repository:VoidType"/>
      </signatures>
    </interface>
    <interface
        name="AudioDB">
      <signatures
          name="QueryDB">
        <returnType
            xsi:type="metaModel.viewType.repository:VoidType"/>
      </signatures>
      <signatures
          name="AddFile">
        <returnType
            xsi:type="metaModel.viewType.repository:VoidType"/>
      </signatures>
    </interface>
    <interface
        name="MediaStore">
      <signatures
          name="download">
        <returnType
            xsi:type="metaModel.viewType.repository:VoidType"/>
      </signatures>
      <signatures
          name="upload">
        <returnType
            xsi:type="metaModel.viewType.repository:VoidType"/>
      </signatures>
    </interface>
  </repositories>
  <enivronments>
    <container
        name="ApplicationServer"
        allocationContext="//@allocationContexts.0 //@allocationContexts.2"
        links="//@enivronments.0/@link.0"/>
    <container
        name="DatabaseServer"
        allocationContext="//@allocationContexts.1"
        links="//@enivronments.0/@link.0"/>
    <link name="ServerDBServer"
        containers="//@enivronments.0/@container.1 //@enivronments.0/@container.0"/>
  </enivronments>
  <systems provides="//@repositories.0/@interface.0">
    <encapsulatedInstances
        name="AssemblyContextWebGUI"
        instanceOf="//@repositories.0/@components.1"
        allocationContext="//@allocationContexts.0">
      <requires
          name="WebGUI"
          delegationConnector="//@systems.0/@delegationConnectors.1"/>
      <provides
          name="WebGUI"/>
    </encapsulatedInstances>
    <encapsulatedInstances
        name="MediaManager"
        instanceOf="//@repositories.0/@components.3">
      <requires
          name="MediaManagerDB"
          assemblyConnector="//@systems.0/@assemblyConnectors.1"/>
      <requires
          name="MediaManagerWatermarking"
          assemblyConnector="//@systems.0/@assemblyConnectors.0"/>
      <provides
          name="MediaManager"/>
    </encapsulatedInstances>
    <encapsulatedInstances
        name="DBCache"
        instanceOf="//@repositories.0/@components.5">
      <requires
          name="DBCache"/>
      <provides
          name="DBCache"
          assemblyConnector="//@systems.0/@assemblyConnectors.1"/>
    </encapsulatedInstances>
    <encapsulatedInstances
        name="DigitalWatermarking"
        instanceOf="//@repositories.0/@components.4">
      <provides
          name="DigitalWatermarking"
          assemblyConnector="//@systems.0/@assemblyConnectors.0"/>
    </encapsulatedInstances>
    <encapsulatedInstances
        name="MediaStore"
        instanceOf="//@repositories.0/@components.2"
        allocationContext="//@allocationContexts.2">
      <requires
          name="MediasstoreDBServer"/>
      <provides
          name="WebGUIMediaStore"/>
    </encapsulatedInstances>
    <encapsulatedInstances
        name="DBServer"
        instanceOf="//@repositories.0/@components.6"
        allocationContext="//@allocationContexts.1">
      <provides
          name="DBServer"
          delegationConnector="//@systems.0/@delegationConnectors.0"/>
    </encapsulatedInstances>
    <delegationConnectors
        xsi:type="metaModel.viewType.assembly:DelegationConnectorProvided"
        linkedInterface="//@repositories.0/@interface.2"
        linkdedRole="//@systems.0/@encapsulatedInstances.5/@provides.0"/>
    <delegationConnectors
        xsi:type="metaModel.viewType.assembly:DelegationConnectorRequired"
        linkedRole="//@systems.0/@encapsulatedInstances.0/@requires.0"
        linkedInterface="//@repositories.0/@interface.3"/>
    <assemblyConnectors
        requiredRole="//@systems.0/@encapsulatedInstances.1/@requires.1"
        providedRole="//@systems.0/@encapsulatedInstances.3/@provides.0"/>
    <assemblyConnectors
        requiredRole="//@systems.0/@encapsulatedInstances.1/@requires.0"
        providedRole="//@systems.0/@encapsulatedInstances.2/@provides.0"/>
  </systems>
  <allocationContexts
      name="Server WebGUI"
      assemblyContext="//@systems.0/@encapsulatedInstances.0"
      container="//@enivronments.0/@container.0"/>
  <allocationContexts
      name="DBServer"
      assemblyContext="//@systems.0/@encapsulatedInstances.5"
      container="//@enivronments.0/@container.1"/>
  <allocationContexts
      name="Server MediaStore"
      assemblyContext="//@systems.0/@encapsulatedInstances.4"
      container="//@enivronments.0/@container.0"/>
</metaModel:ComponentBasedSystem>
